<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="back" layoutX="-2.0" layoutY="-2.0" prefHeight="715.0" prefWidth="245.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.chocopi.controller.admin.abstractAdminSideBar">
    <children>
        <ImageView fitHeight="120.0" fitWidth="120.0" layoutX="62.0" layoutY="32.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../images/identify/logo.png" />
            </image>
        </ImageView>
        <VBox layoutX="12.0" layoutY="181.0" prefHeight="234.0" prefWidth="220.0" spacing="18.0">
            <children>
                <Button id="dashboard" fx:id="home" mnemonicParsing="false" onAction="#handleHomeClick" prefHeight="42.0" prefWidth="220.0" text="Dashboard">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Button>
                <Button id="user" fx:id="student" mnemonicParsing="false" onAction="#handleStudentClick" prefHeight="42.0" prefWidth="220.0" text="User">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Button>
                <Button id="book" fx:id="book" mnemonicParsing="false" onAction="#handleBookClick" prefHeight="42.0" prefWidth="220.0" text="Book">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Button>
                <Button id="user" fx:id="issuedBook" layoutX="10.0" layoutY="70.0" mnemonicParsing="false" onAction="#handleIssuedBookClick" prefHeight="42.0" prefWidth="220.0" text="Issued Book">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Button>
                <Button id="logout" fx:id="logout" mnemonicParsing="false" onAction="#handleLogoutClick" prefHeight="42.0" prefWidth="220.0" text="Log out">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Button>
            </children>
        </VBox>
    </children>
</AnchorPane>
